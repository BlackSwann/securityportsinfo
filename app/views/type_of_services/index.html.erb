<h1>Listing type_of_services</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_of_services.each do |type_of_service| %>
      <tr>
        <td><%= type_of_service.name %></td>
        <td><%= type_of_service.description %></td>
        <td><%= link_to 'Show', type_of_service %></td>
        <td><%= link_to 'Edit', edit_type_of_service_path(type_of_service) %></td>
        <td><%= link_to 'Destroy', type_of_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type of service', new_type_of_service_path %>
