<h1>Listing service_version_sos</h1>

<table>
  <thead>
    <tr>
      <th>Version so</th>
      <th>Service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_version_sos.each do |service_version_so| %>
      <tr>
        <td><%= service_version_so.version_so_id %></td>
        <td><%= service_version_so.service_id %></td>
        <td><%= link_to 'Show', service_version_so %></td>
        <td><%= link_to 'Edit', edit_service_version_so_path(service_version_so) %></td>
        <td><%= link_to 'Destroy', service_version_so, method: :delete, data: { confirm: 'Are you sure?' } %></td>
